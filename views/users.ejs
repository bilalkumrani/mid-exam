<!DOCTYPE html>
<html lang="en">
  <head>
    <title>All users</title>
  </head>
  <body>
    <table style="border: 2px solid;" >
      <tr>
        <th></th>
        <th>Name</th>
        <th>Email</th>
        <th>phone</th>
        <th>city</th>
        <th>postcode</th>
      </tr>
      <%for(let i of data){%>
      <tr style="border: 2px solid;">
        <td>
          <img style="height: 30px" src="<%=i.img%>" alt="" /> <td><%=i.name%></td>
        </td>
        <td><%=i.email%></td>
        <td><%=i.phone%></td>
        <td><%=i.city%></td>
        <td><%=i.zip%></td>
      </tr>

      <%}%>
    </table>
  </body>
</html>
